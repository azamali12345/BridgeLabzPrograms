<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:aop="http://www.springframework.org/schema/aop"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
           http://www.springframework.org/schema/aop
           http://www.springframework.org/schema/aop/spring-aop-2.5.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-2.5.xsd">
           
	<bean id="azam" class="com.bridgelabz.constructor.Student">
	<constructor-arg name="studentName" value="Azam Ali" type="java.lang.String"/>
	<constructor-arg name="id" value="1" type="int"/>
	</bean>
	
	<bean id="ali" class="com.bridgelabz.constructor.Student">
	<constructor-arg name="studentName" value="Syed Ali"/>
	<constructor-arg name="id" value="2" type="int"/>
	</bean>
</beans>